{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "前端",
            "type": "lua",
            "request": "launch",
            "stopOnEntry": true,
            "cwd": "${workspaceRoot}/script/",
            "path": ".\\?.lua;.\\?\\init.lua",
            "cpath": "${workspaceRoot}/bin/?.dll;",
            "program": "${workspaceRoot}/script/main.lua",
            "console": "utf8"
        },
        {
            "name": "后端",
            "type": "lua",
            "request": "launch",
            "program": "${workspaceRoot}/script/map.lua",
            "stopOnEntry": true,
            "cwd": "${workspaceRoot}/script/",
            "path": ".\\?.lua;.\\?\\init.lua",
            "cpath": "${workspaceRoot}/bin/?.dll;",
            "arg0": "${workspaceRoot}/bin/w3x2lni.exe",
            "arg": [
                "${workspaceRoot}/FinaldayDefendApocalypse.Ver3.3.w3x",
            ],
            "console": "utf8"
        },
        {
            "name": "预生成",
            "type": "lua",
            "request": "launch",
            "program": "${workspaceRoot}/script/prebuilt/init.lua",
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}/script/",
            "path": ".\\?.lua;.\\?\\init.lua",
            "cpath": "${workspaceRoot}/bin/?.dll;",
            "arg0": "${workspaceRoot}/bin/w3x2lni.exe",
            "console": "ansi"
        },
    ]
}
